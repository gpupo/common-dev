{
    "name": "gpupo/common-dev",
    "description": "Common PHP tool for require-dev env",
    "homepage": "https://opensource.gpupo.com/common-dev/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common", "development"],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "phpunit/phpunit": "^8.5||^9.0",
        "symfony/var-dumper": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/dotenv": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonDev\\": "src",
            "Gpupo\\CommonDev\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "gpupo/common-sdk": "*"
    },
    "replace": {
        "gpupo/common-dev": "*"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
