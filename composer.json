{
    "name": "gpupo/common-dev",
    "description": "Common PHP tool for require-dev env",
    "homepage": "https://opensource.gpupo.com/common-dev/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["common"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "phpunit/phpunit": "^8.5",
        "phan/phan": "^1.3",
        "vimeo/psalm": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\CommonDev\\": "src",
            "Gpupo\\CommonDev\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "symfony/dotenv": "^4"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
